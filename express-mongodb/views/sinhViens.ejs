<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1  align="center">Danh sách sinh viên</h1>
    <table border="1" align="center" >
        <thead>
            <tr>
                <th>ID</th>
                <th>Name</th>
            </tr>
        </thead>
        <tbody>
            <% sinhvien.forEach(function(sinhvien){ %>
            <tr>
                <td><%= sinhvien.id %></td>
                <td><%= sinhvien.name %></td>
            </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>